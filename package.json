{
  "name": "coding-is-magic",
  "version": "1.0.0",
  "description": "A game to teach code using Unity and Google Blockly.",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "npm run unity-prep && firebase deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "# lint-staged # un-comment to enable",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "seed": "node script/seed.js",
    "start": "firebase serve",
    "which": "which firebase",
    "start-dev": "npm run unity-prep && npm run build-client-watch & firebase serve",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "unity-prep": "cp -r Build public/Build && rsync public/Build/Build/* public/Build"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "blockly": "^1.0.0",
    "compression": "^1.7.3",
    "firebase": "^5.5.8",
    "firebase-admin": "^6.1.0",
    "firebase-cli": "^1.2.0",
    "firebase-tools": "^6.1.1",
    "firebaseui": "^3.4.1",
    "google-blockly": "^1.0.0",
    "history": "^4.6.3",
    "jsdom": "^11.3.0",
    "node-blockly": "^1.0.35",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-blockly-component": "^3.1.1",
    "react-blockly-drawer": "^1.6.6",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-spring": "^6.1.9",
    "react-unity-webgl": "^7.0.7",
    "xmlshim": "git://github.com/eyz/node-xmlshim.git#be6220b53809b2c5a84be25c17e56bc4a12ba411"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.54",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-function-sent": "7.0.0-beta.54",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.54",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-beta.54",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/register": "^7.0.0-beta.55",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^3.5.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.4.2",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  }
}
